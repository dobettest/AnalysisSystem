{
  "presets": [
    [
      "@babel/preset-env"
    ],
    "@vue/babel-preset-jsx"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "import",
      {
        "libraryName": "ant-design-vue",
        "libraryDirectory": "es",
        "style": true
      }
    ] // `style: true` 会加载 less 文件
  ]
}